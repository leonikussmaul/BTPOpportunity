{
    "sap.app": {
        "id": "my.new.card",
        "type": "card",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },

    "sap.card": {
		"type": "List",
		"header": {
			"title": "Quick Links",
			"actions": [
				{
					"type": "Navigation",
					"parameters": {
						"url": "/quickLinks"
					}
				}
			]
		},
		"content": {
			"data": {
				"json": [
					
					{
						"Name": "Projects",
						"icon": "sap-icon://meeting-room",
						"url": "/app/opportunity/webapp/index.html#/ProjectOverview"
					},

					{
						"Name": "Calendar",
						"icon": "sap-icon://appointment-2",
						"url": "/app/opportunity/webapp/index.html#/Calendar"
					},
					{
						"Name": "Team",
						"icon": "sap-icon://employee-lookup",
						"url": "/app/opportunity/webapp/index.html#/Team"
					},
					{
						"Name": "Services Catalog",
						"icon": "sap-icon://education",
						"url": "https://servicescatalog.cvdp3eof-dbsservic1-p1-public.model-t.cc.commerce.ondemand.com/"
					},
					{
						"Name": "Analytics Store",
						"icon": "sap-icon://business-objects-experience",
						"url": "https://eas.sap.com/astore/ui/index.html#assets"
					},

					{
						"Name": "BTP SharePoint",
						"icon": "sap-icon://idea-wall",
						"url": "https://sap.sharepoint.com/sites/201967"
					}
				]
			},
			"maxItems": 6,
			"item": {
				"icon": {
					"src": "{icon}"
				},
				"title": "{Name}",
				"actions": [
					{
						"type": "Navigation",
						"target": "_self",
						"enabled": "{= ${url}}",
						"parameters": {
							"url": "{url}"
						}
					}
				]
			}
		}
	}
}