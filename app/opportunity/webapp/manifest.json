{
    "_version": "1.49.0",
    "sap.app": {
        "id": "opportunity.opportunity",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.9.4",
            "toolsId": "7edde770-382b-473c-b00f-b677e246fb50"
        },
        "dataSources": {
            "mainService": {
                "uri": "/v2/Opportunity/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotations"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0",
                    "defaultBindingMode": "TwoWay"
                }
            },
            "userapi": {
                "uri": "/user-api/attributes",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "odataVersion": "2.0"
                }
            },
            "V4Service": {
                "uri": "/Opportunity/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata2.xml",
                    "odataVersion": "4.0"
                }
            },
            "annotations": {
                "type": "ODataAnnotation",
                "uri": "./annotations.xml",
                "settings": {
                    "localUri": "./annotations.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.124.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "components": {
            "chatbot": {}
          },
          "componentUsages": {
            "chatbot": {
              "name": "chatbot",
              "componentData": {
                "renderButton": true
              }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "opportunity.opportunity.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": false,
                "settings": {
                    "disableHeadRequestForToken": true,
                    "autoExpandSelect": true
                }
            },
            "V4Service": {
                "dataSource": "V4Service",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "userapi": {
                "dataSource": "userapi",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "opportunity.opportunity.view",
                "controlId": "app",
                "clearControlAggregation": true
            },
            "routes": [
                {
                    "name": "Overview",
                    "pattern": ":?query:",
                    "target": [
                        "Overview"
                    ]
                },
                {
                    "name": "OpportunityReport",
                    "pattern": "OpportunityReport",
                    "target": [
                        "OpportunityReport",
                        "OpportunityDetail"
                    ]
                },
                {
                    "name": "OpportunityDetail",
                    "pattern": "OpportunityDetail/{opportunityID}/{layout}",
                    "target": [
                        "OpportunityReport",
                        "OpportunityDetail"
                    ]
                },
                {
                    "name": "GenieAIMain",
                    "pattern": "GenieAIMain/{type}",
                    "target": [
                        "GenieAIMain",
                        "GenieAIDetail"
                    ]
                },
                {
                    "name": "GenieAIDetail",
                    "pattern": "GenieAIDetail/{type}/{workshopID}/{layout}",
                    "target": [
                        "GenieAIMain",
                        "GenieAIDetail"
                    ]
                },
                {
                    "name": "GenieAIWorkshop",
                    "pattern": "GenieAIWorkshop/{type}/{workshopType}/{workshopID}/{layout}",
                    "target": [
                        "GenieAIMain",
                        "GenieAIWorkshop"
                    ]
                },
                {
                    "name": "TeamChart",
                    "pattern": "TeamChart",
                    "target": "TeamChart"
                },
                {
                    "name": "IndividualEngagement",
                    "pattern": "IndividualEngagement/{inumber}",
                    "target": "IndividualEngagement"
                },
                {
                    "name": "TeamEngagement",
                    "pattern": "TeamEngagement",
                    "target": "TeamEngagement"
                },
                {
                    "name": "Calendar",
                    "pattern": "Calendar",
                    "target": "Calendar"
                },
                {
                    "name": "chatbot",
                    "pattern": "chatbot:?query:",
                    "target": {
                      "name": "chatbot",
                      "prefix": "i"
                    }
                  }
            ],
            "targets": {
                "Overview": {
                    "level": 0,
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "Overview",
                    "viewName": "Overview",
                    "controlAggregation": "beginColumnPages"
                },
                "OpportunityReport": {
                    "level": 1,
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "OpportunityReport",
                    "viewName": "OpportunityReport",
                    "controlAggregation": "beginColumnPages"
                },
                "OpportunityDetail": {
                    "level": 1,
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "OpportunityDetail",
                    "viewName": "OpportunityDetail",
                    "controlAggregation": "midColumnPages"
                },
                "GenieAIMain": {
                    "level": 1,
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "GenieAIMain",
                    "viewName": "GenieAIMain",
                    "controlAggregation": "beginColumnPages"
                },
                "GenieAIDetail": {
                    "level": 1,
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "GenieAIDetail",
                    "viewName": "GenieAIDetail",
                    "controlAggregation": "midColumnPages"
                },
                "GenieAIWorkshop": {
                    "level": 1,
                    "viewType": "XML",
                    "transition": "slide",
                    "viewId": "GenieAIWorkshop",
                    "viewName": "GenieAIWorkshop",
                    "controlAggregation": "midColumnPages"
                },
                "Calendar": {
                    "viewType": "XML",
                    "transition": "slide",
                    "controlAggregation": "beginColumnPages",
                    "viewId": "Calendar",
                    "viewName": "Calendar",
                    "viewLevel": 1
                },
                "TeamChart": {
                    "viewType": "XML",
                    "transition": "slide",
                    "controlAggregation": "beginColumnPages",
                    "viewId": "TeamChart",
                    "viewName": "TeamChart",
                    "viewLevel": 1
                },
                "IndividualEngagement": {
                    "viewType": "XML",
                    "transition": "slide",
                    "controlAggregation": "beginColumnPages",
                    "viewId": "IndividualEngagement",
                    "viewName": "IndividualEngagement",
                    "viewLevel": 2
                },
                "TeamEngagement": {
                    "viewType": "XML",
                    "transition": "slide",
                    "controlAggregation": "beginColumnPages",
                    "viewId": "TeamEngagement",
                    "viewName": "TeamEngagement",
                    "viewLevel": 1
            },
             "chatbot": {
                "controlAggregation": "beginColumnPages",
                "viewLevel": 1,
                "type": "Component",
                "usage": "chatbot",
                "title": "chatbot",
                "id": "chatbot"
              }
            }
        },
        "rootView": {
            "viewName": "opportunity.opportunity.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {}
            }
        }
    }
}