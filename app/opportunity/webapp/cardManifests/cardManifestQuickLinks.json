{
    "sap.app": {
        "id": "my.new.card",
        "type": "card",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },

    "sap.card": {
		"type": "List",
		"header": {
			"title": "Quick Links",
			"actions": [
				{
					"type": "Navigation",
					"parameters": {
						"url": "/quickLinks"
					}
				}
			]
		},
		"content": {
			"data": {
				"json": [
					{
						"Name": "Opportunities",
						"icon": "sap-icon://target-group",
						"url": "/opportunity/webapp/index.html#/MainReport"
					},
					{
						"Name": "Tasks",
						"icon": "sap-icon://activities",
						"url": "/opportunity/webapp/index.html#/TasksReport"
					},
					{
						"Name": "Analytics",
						"icon": "sap-icon://filter-analytics",
						"url": "/opportunity/webapp/index.html#/ChartPage"
					},

					{
						"Name": "Calendar",
						"icon": "sap-icon://appointment-2",
						"url": "/opportunity/webapp/index.html#/Calendar"
					},
					{
						"Name": "Team",
						"icon": "sap-icon://family-protection",
						"url": "/opportunity/webapp/index.html#/Team"
					}
				]
			},
			"maxItems": 5,
			"item": {
				"icon": {
					"src": "{icon}"
				},
				"title": "{Name}",
				"actions": [
					{
						"type": "Navigation",
						"enabled": "{= ${url}}",
						"parameters": {
							"url": "{url}"
						}
					}
				]
			}
		}
	}
}