{
	"_version": "1.25.0",
	"sap.app": {
		"id": "card.explorer.sample.table.card.employees",
		"title": "Employees",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"type": "card"
	},
	"sap.ui": {
		"deviceTypes":{
			"desktop": true,
			"phone": true,
			"tablet": true
		},
		"technology": "UI5"
	},
	"sap.card": {
		"type": "Table",
		"header": {
			"title": "Go-Live",
			"subTitle": "Current Go-Live's",
			"status": {
				"text": "by most recent"
			}
		},
		"content": {
			"maxItems": 5,
            "data": {
                "request": {
                    "url": "/Opportunity/teamProjects",
                    "method": "GET",
                    "parameters": {
                        "$format": "json",
                        "$filter": "status eq 'Go-Live'",
						"$orderby": "lastUpdated desc"
                    }
                },
                "updateInterval": 300,
                "path": "/value"
            },
			"row": {
				"columns": [
					
					{
						"title": "Project",
						"value": "{account}",
                        "identifier": true,
						"width": "25%"
					},
                    {
						"title": "Owner",
                        "value": "{primaryContact}",
						"icon": {
							"src": "{Icon}",
							"shape": "Circle"
						},
						"width": "20%"
					},
					{
						"title": "Status",
						"value": "{status}",
						"width": "20%"
					},
					{
						"title": "Engagement",
						"value": "{percentage} %",
                        "width": "35%"
					}
				]
			}
		}
	}
}