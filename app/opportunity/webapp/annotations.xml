<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0"
	xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
	xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
	xmlns:sap="http://www.sap.com/Protocols/SAPData">
	<edmx:Reference
		xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
		<edmx:Include Namespace="com.sap.vocabularies.Common.v1"
			Alias="Common"/>
	</edmx:Reference>
	<edmx:Reference
		xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
		<edmx:Include Namespace="com.sap.vocabularies.UI.v1"
			Alias="UI"/>
	</edmx:Reference>
	<edmx:DataServices m:DataServiceVersion="2.0">

	
		<Schema Namespace="EPM_REF_APPS_PROD_MAN_SRV"
			sap:schema-version="0"
			xmlns="http://schemas.microsoft.com/ado/2008/09/edm">





			
			
			<Annotations Target="OpportunityService.opportunityHeaderCopy"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">
				
				<Annotation Term="com.sap.vocabularies.UI.v1.Chart"
					Qualifier="Pie_Chart">
					<Record>
						<PropertyValue Property="ChartType"
							EnumMember="com.sap.vocabularies.UI.v1.ChartType/Pie" />
						<PropertyValue Property="Dimensions">
							<Collection>
								<PropertyPath>marketUnit</PropertyPath>
							</Collection>
						</PropertyValue>
						<PropertyValue Property="Measures">
							<Collection>
								<PropertyPath>opportunityValue</PropertyPath>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
				<Annotation Term="com.sap.vocabularies.Common.v1.SemanticObject" String="SemanticObjectCategory"/>
				<Annotation Term="com.sap.vocabularies.UI.v1.Chart"
					Qualifier="Quarter_Chart">
					<Record>
						<PropertyValue Property="ChartType"
							EnumMember="com.sap.vocabularies.UI.v1.ChartType/Line" />
						<PropertyValue Property="Dimensions">
							<Collection>
								<PropertyPath>opportunityClosedQuarter</PropertyPath>
							</Collection>
						</PropertyValue>
						<PropertyValue Property="Measures">
							<Collection>
								<PropertyPath>opportunityValue</PropertyPath>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>

			

				<Annotation Term="com.sap.vocabularies.Common.v1.SemanticObject" String="SemanticObjectCategory"/>
			</Annotations>
			<Annotations Target="OpportunityService.opportunityHeaderCopy"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.Common.v1.SemanticObject"
					String="SemanticObjectCategory"/>
			</Annotations>



	
			<Annotations Target="OpportunityService.teamForecast"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">
				
					<Annotation Term="com.sap.vocabularies.UI.v1.PresentationVariant"
					Qualifier="Name_Sort">
					<Record>
						<PropertyValue Property="Visualizations">
							<Collection>
								<AnnotationPath>@com.sap.vocabularies.UI.v1.Chart</AnnotationPath>
							</Collection>
						</PropertyValue>
						<PropertyValue Property="SortOrder">
							<Collection>
								<Record>
									<PropertyValue Property="Property">
										<PropertyPath>order</PropertyPath>
									</PropertyValue>
									<PropertyValue Property="Descending">
										<Bool>true</Bool>
									</PropertyValue>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>

				
			</Annotations>

			<!-- <Annotations Target="OpportunityService.opportunityHeaderCopy"
				xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.Common.v1.SemanticObject"
					String="SemanticObjectCategory"/>
			</Annotations> -->


		</Schema>
	</edmx:DataServices>
</edmx:Edmx>
