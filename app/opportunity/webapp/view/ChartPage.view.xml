<mvc:View
    controllerName="opportunity.opportunity.controller.ChartPage"
    id="view1"
    xmlns:u="sap.uxap"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:form="sap.ui.layout.form"
    xmlns:f="sap.f"
    xmlns:l="sap.ui.layout"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
    xmlns:smartchart="sap.ui.comp.smartchart"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:table="sap.ui.table"
    xmlns:plugins="sap.ui.table.plugins"
    xmlns:layout="sap.ui.layout"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:sl="sap.ui.comp.navpopover"
    xmlns:data="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    
>
    <f:DynamicPage
        id="ChartPage"
        title="Opportunity Chart"
        backgroundDesign="List"
    >
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title
                        text="Analytics &amp; Overview"
                        id="headerTitle"
                    />
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <f:content>
            <IconTabBar class="sapUiResponsiveContentPadding">
                <items>
                    <IconTabFilter text="EMEA N Overview">
                        <content>
                            <smartchart:SmartChart
                                id="smartChart1"
                                enableAutoBinding="true"
                                entitySet="opportunityHeaderCopy"
                                beforeRebindChart="onBeforeRebindChart"
                                useVariantManagement="false"
                                useChartPersonalisation="true"
                                header="Value per Opportunity"
                                showFullScreenButton="true"
                                selectionMode="Multi"
                                showChartTooltip="true"
                                showDrillBreadcrumbs="true"
                                showDetailsButton="false"
                                showDrillButtons="true"
                                showSemanticNavigationButton="true"
                                showDownloadButton="true"
                            />
                        </content>
                    </IconTabFilter>

                     <IconTabFilter text="Team Utilization">
                        <content>
                            <!-- <smartchart:SmartChart
                                id="smartChart2"
                                enableAutoBinding="true"
                                entitySet="opportunityHeaderCopy"
                                useVariantManagement="false"
                                useChartPersonalisation="true"
                                header="Value per Market Unit"
                                showFullScreenButton="true"
                                showChartTooltip="true"
                                showDrillBreadcrumbs="false"
                                showDetailsButton="false"
                                showDrillButtons="false"
                                showSemanticNavigationButton="false" 
                                showDownloadButton="true"
                                data:chartQualifier="Pie_Chart"

                            /> -->

                             <smartchart:SmartChart id ="smartChart2" enableAutoBinding="true" initialized="onInitChart"
                                    entitySet="teamForecastCopy" 
                                    useVariantManagement="false"
			                        useChartPersonalisation="true" beforeRebindChart="onRebindUtilizationChart"
			                        header="Team Average over the Year" showFullScreenButton="true" selectionMode="Multi"
			                        showChartTooltip="true" showDrillBreadcrumbs="false"
			                        showDetailsButton="false" showDrillButtons="true" 
			                        showSemanticNavigationButton="true"  
                                    showDownloadButton="true" data:presentationVariantQualifier="Name_Sort">
                                    	
            </smartchart:SmartChart>
                        </content>
                    </IconTabFilter>

                    <IconTabFilter text="Closing Quarter Overview">
                        <content>
                            <smartchart:SmartChart
                                id="smartChart3"
                                enableAutoBinding="true"
                                entitySet="opportunityHeaderCopy"
                                useVariantManagement="false"
                                useChartPersonalisation="true"
                                header="Value per Q. Closed"
                                showFullScreenButton="true"
                                showChartTooltip="true"
                                showDrillBreadcrumbs="false"
                                showDetailsButton="false"
                                showDrillButtons="false"
                                showSemanticNavigationButton="false" 
                                showDownloadButton="true"
                                data:chartQualifier="Quarter_Chart"
                                beforeRebindChart="onBeforeRebindQuarterChart"

                            />
                        </content>
                    </IconTabFilter>

                </items>
            </IconTabBar>
        </f:content>
    </f:DynamicPage>
</mvc:View>
