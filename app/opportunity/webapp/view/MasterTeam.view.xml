<mvc:View
    xmlns="sap.m" 
    xmlns:f="sap.f"  
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns:l="sap.ui.layout"
    displayBlock="true" 
    controllerName="opportunity.opportunity.controller.MasterTeam"
    height="100%"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:tnt="sap.tnt">



             <VBox>
       

 <App id="app">
            <f:FlexibleColumnLayout
                 id="flexiblecolumnlayout"
                layout="{FlexColLayoutModel>/layout}"
                backgroundDesign="Solid"
                stateChange="onStateChanged">
                <f:beginColumnPages> 
                    <mvc:XMLView id="beginView" viewName="opportunity.opportunity.view.App" />
                </f:beginColumnPages>
            </f:FlexibleColumnLayout>
        </App>
     </VBox>
            
</mvc:View>
